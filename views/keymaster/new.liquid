{% layout "layout.liquid" %} {% block content %}
<div>
  <form action="/keymaster" method="post">
    <fieldset>
      <legend>Enter your credentials</legend>
     
      <label>
        Account Name:<input type="text" name="acctName" placeholder="" />
      </label>
      <label>
        User Name:<input type="text" name="userName" placeholder="username@service" />
      </label>
        <label>
        Password:<input type="text" name="passwordKey" placeholder="Password" />
      </label>
      <label> SAVE:<input type="checkbox" name="saved" /> </label>
    </fieldset>
    <input type="submit" value="Save" />
  </form>
</div>
{% endblock %}